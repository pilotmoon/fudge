{
  "name": "@pilotmoon/fudge",
  "module": "index.ts",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "check": "bunx tsc --noEmit",
    "build": "bun run check && bun build index.ts --outfile index.js --target node --external case-anything --external js-yaml --external fast-plist --external valibot --external emoji-regex",
    "watch": "bun run build --watch"
  },
  "devDependencies": {
    "@types/js-yaml": "4.0.9",
    "axios": "1.6.8",
    "bun-types": "latest",
    "fast-stable-stringify": "1.0.0"
  },
  "peerDependencies": {
    "typescript": "5.4.2"
  },
  "dependencies": {
    "case-anything": "2.1.13",
    "emoji-regex": "10.3.0",
    "fast-plist": "0.1.3",
    "js-yaml": "4.1.0",
    "valibot": "0.30.0"
  }
}
