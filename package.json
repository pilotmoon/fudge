{
  "name": "@pilotmoon/fudge",
  "module": "index.ts",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "check": "bunx tsc --noEmit",
    "build": "bun run check && bun build index.ts --outfile index.js --target node --external case-anything --external zod --external js-yaml --external fast-plist",
    "watch": "bun run build --watch"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "bun-types": "latest"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "case-anything": "2.1.13",
    "fast-plist": "0.1.3",
    "js-yaml": "4.1.0",
    "zod": "3.22.4"
  }
}
